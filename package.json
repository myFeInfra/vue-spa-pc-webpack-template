    "stylelint-order": "^7.0.0",
    "swc": "^1.0.11",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unocss": "66.1.0-beta.12",
    "unplugin-auto-import": "^19.1.2",
